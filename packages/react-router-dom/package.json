{
  "name": "@react-file-based-routing/react-router-dom",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "scripts": {
    "dev": "npm run build -- --watch",
    "lint": "eslint --fix src/**",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --config ../../jest.config.ts",
