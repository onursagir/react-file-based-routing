  "keywords": [
    "react",
    "file",
    "based",
    "routing",
    "react-router-dom"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fast-glob": "^3.2.11",
    "remove": "^0.1.5"
  },
  "peerDependencies": {
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "cpy-cli": "^4.1.0"
  }
}
